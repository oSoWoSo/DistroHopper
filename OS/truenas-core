function releases_truenas-core() {
    echo 13.0 12.0
}

function get_truenas-core() {
    local HASH=""
    local ISO=""
    local URL=""
    local DLINFO="https://www.truenas.com/download-truenas-core/"
    URL=$(wget -q ${DLINFO} -O- | grep -o "\"https://.*${RELEASE}.*\.iso\""|cut -d\" -f2)

    HASH=$(wget -q ${URL}.sha256 -O- | cut_1)
    echo "${URL} ${HASH}"
}
